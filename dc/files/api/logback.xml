<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <withJansi>true</withJansi>
        <encoder>
            <pattern>{"date":"%d{yyyy-MM-dd_HH:mm:ss.SSS}", "thread": "%thread", "level": "%-5level", "msg": %msg}%n</pattern>
        </encoder>
    </appender>

    <root level="DEBUG">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
